<div class="fixedTitle">
  <div class="tbody">
    <div class="trow header">
      <div class="tcell stt">#</div>
      <div class="tcell evaluation_template_name title">
        <%= t "evaluation_templates.label.name" %>
        <%= link_to filter_datas_path(type: :name,
          filter_type: :evaluation_templates),
          remote: true, class: "open-select", data: {name: "f-evaluation_template_name"} do %>
          <i class="fa ic ic-filter evaluation_template_name"></i>
        <% end %>
      </div>
      <div class="clear-both">
        <div class="side-bar">
        </div>
      </div>
    </div>
  </div>
</div>

<header class="fixedTable-header">
  <div id="table-header" class="scroll-table">
    <div class="tbody">
      <div class="trow header">
        <div class="tcell action-edit"></div>
        <div class="tcell action-delete"></div>
      </div>
    </div>
  </div>
</header>
