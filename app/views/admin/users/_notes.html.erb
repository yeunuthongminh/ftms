<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title"><%= t "user.notes" %></h3>
    <strong><%= link_to t("notes.add"), "#", data: {toggle: "modal",
      target: "#new-note-modal"},
      class: "glyphicon glyphicon-plus glyphicon pull-right" %>
    </strong>
  </div>

  <div class="box-body">
    <table class="table table-hover note-list">
      <%= render partial: "admin/notes/note", collection: @supports.notes %>
    </table>
  </div>
</div>

<%= render "admin/notes/note_modal" %>
<div class="note-modal"></div>
